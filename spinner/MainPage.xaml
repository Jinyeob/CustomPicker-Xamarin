<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"  x:Class="spinner.MainPage">

    <StackLayout Spacing="20" Margin="10, 15">
      
      <Picker x:Name="picker"
              Title="Select a Category">

  <Picker.ItemsSource>
    <x:Array Type="{x:Type x:String}">
        <x:String>General</x:String>
        <x:String>Health</x:String>
      <x:String>Career</x:String>
      <x:String>Relationship</x:String>
      <x:String>Learning</x:String>
      <x:String>Enjoyment</x:String>
      <x:String>Wealth</x:String>
      <x:String>Travel</x:String>
    </x:Array>
  </Picker.ItemsSource>
</Picker>
    <Button Text="ListView Page" Clicked="OnOpenListViewPage"></Button>

  </StackLayout>

    


</ContentPage>